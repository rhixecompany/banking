{
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "printWidth": 80,
    "tabWidth": 2,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "tailwindFunctions": [
        "tw",
        "clsx",
        "cva",
        "cn"
    ],
    "tailwindAttributes": [
        "className"
    ],
    "tailwindStylesheet": "./app/global.css"
}